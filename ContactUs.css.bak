body{
   margin:0px;
   background-image:url(noo.jpg) ; 
    background-size: cover;
     opacity:0.9;
}

.allPage{
	
   display:grid;
   grid-template-areas: 
    'header header header header header header' 
    'sec sec sec sec sec sec ' 
    
	
	
}
.Header{
	grid-area:header;
	border:2px solid white ;
	background:#8f76a3;
	padding-right:30px;
	margin:30px;
	opacity:0.9;

}

.sec{
	grid-area:sec;
	
	height:500px;
	font-size:20px;
	margin-bottom:30px;
  
}
.sec1{
	 float:left;
	 height:297px;
	 padding-right:80px;
	 padding-bottom:205px;
	 padding-left:30px;
	 background:#8f76a3;
	color:#FCF3CF ;
	margin-left:30px;
	border:2px solid white ;
	opacity:0.8;
}
.sec2{
	float:right;
	height:297px;
	padding:90px;
	padding-bottom:115px;
	color:#FCF3CF ;
	background:#8f76a3;
	margin-right:30px;
	border:2px solid white ;
	opacity:0.8;
}


.contact  {   
	font-size:12px;
	text-align:left;
	
}

 img{
	 	border-radius:50%;
    	float:left;
		margin-right:10px;
	
}


ul {
  list-style-type:none;
  float:right;
}
li{
	display:inline-block;
	padding-top:40px;
